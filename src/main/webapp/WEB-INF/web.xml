<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>CountryList</servlet-name>
        <servlet-class>kz.iitu.CountryList</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CountryList</servlet-name>
        <url-pattern>/CountryList</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Languages</servlet-name>
        <servlet-class>kz.iitu.Languages</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Languages</servlet-name>
        <url-pattern>/Languages</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Information</servlet-name>
        <servlet-class>kz.iitu.Information</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Information</servlet-name>
        <url-pattern>/Information</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Flag</servlet-name>
        <servlet-class>kz.iitu.Flag</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Flag</servlet-name>
        <url-pattern>/Flag</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Currency</servlet-name>
        <servlet-class>kz.iitu.Currency</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Currency</servlet-name>
        <url-pattern>/Currency</url-pattern>
    </servlet-mapping>

</web-app>